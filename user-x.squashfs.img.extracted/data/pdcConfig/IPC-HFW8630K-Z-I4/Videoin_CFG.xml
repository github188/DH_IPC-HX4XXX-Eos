<?xml version="1.0" encoding="UTF-8"?> 
<Videoin_CFG>
    <!--能力值-->
    <pVideoinCaps>
        <!--视频输入传感器最大水平感光分辨率，或者支持的外部视频输入最大分辨率-->
        <maxWidth>3072</maxWidth>
        <!--视频输入传感器最大垂直感光分辨率，或者支持的外部视频输入最大分辨率-->
        <maxHeight>2048</maxHeight>
        <!--视频输入支持的最高视频负载码流kbps，按16bpp计算，非binning模式下-->
        <ladenBitrate>2949120</ladenBitrate>
        <!--水平像素叠加功能掩码，从最低到最高每位依次对应是否支持2,3,4...像素叠加-->
        <horizontalBinning>0</horizontalBinning>
        <!--垂直像素叠加功能掩码，从最低到最高每位依次对应是否支持2,3,4...像素叠加-->
        <verticalBinning>0</verticalBinning>
        <!--是否支持画面缩小，不包括binning模式-->
        <downScaling>0</downScaling>
        <!--是否支持画面放大-->
        <upScaling>0</upScaling>
        <!--曝光速度控制等级数，支持的曝光等级数，包括自动曝光。0-不支持曝光控制，1-只支持自动曝光，2..n-曝光等级数。速度在getExposureSpeeds接口中取到。-->
        <exposure>16</exposure>
        <!--是否支持背光补偿，，0表示不支持背光补偿，1支持，2-支持区域调节-->
        <backlight>2</backlight>
        <!--是否支持日夜颜色自动转换，0-不支持，1-支持-->
        <dayNightColor>1</dayNightColor>
        <!--是否支持图像镜像，0-不支持，1-支持-->
        <mirror>1</mirror>
        <!--是否支持图像翻转，0-不支持，1-支持-->
        <flip>1</flip>
        <!--是否支持白平衡，0-不支持，1-支持自动白平衡，2-也支持预置白平衡，3-也支持自定义白平衡,  4- 手动选择白平衡基准点-->
        <whiteBalance>3</whiteBalance>
        <!--是否支持增益控制-->
        <gain>1</gain>
        <!--是否支持自动增益-->
        <gainAuto>1</gainAuto>
        <!--支持最多的区域覆盖个数。-->
        <coverCount>3</coverCount>
        <!--是否支持区域覆盖 0-不支持，1-支持实时的区域覆盖， 即设置区域时速度较快，可以实时调用， 2-支持非实时的区域覆盖，即设置区域时速度较慢，只能在用户确定区域后调用。-->
        <coverType>1</coverType>
        <!--支持最多的标题叠加的个数。-->
        <titleCount>2</titleCount>
        <!--是否支持光圈调节(手动)-->
        <iris>1</iris>
        <!--是否支持光圈自动调节-->
        <irisAuto>0</irisAuto>
        <!--是否支持红外补偿，0-不支持，1-支持-->
        <infraRed>0</infraRed>
        <!--是否支持颜色调节-->
        <setcolor>1</setcolor>
        <!--前端支持输出分辨率数目，具体分辨率数值使用getFormatCaps接口函数获取。如果值为0，表示前端格式采用编码的格式(比如分辨率、帧率)，不需要限制。-->
        <formatCount>7</formatCount>
        <!--是否支持分辨率顺时针或逆时针旋转90°，0-不支持，1-支持-->
        <rotate90>1</rotate90>
        <!--支持的视频输入信号格式掩码，\see VideoSignalFormat-->
        <signalFormatMask>0</signalFormatMask>
        <!--通道同步是指由于前端AD的限制，同一个芯片上的某个码流(主码流或辅码流)的多个通道的分辨率必须一致。-->
        <syncChipChannels>0</syncChipChannels>
        <!--通道所在的芯片号-->
        <chipID>0</chipID>
        <!--是否支持多套选项，0-不支持，1-支持白天夜晚两套选项-->
        <multiOptions>0</multiOptions>
        <!--是否支持抓图单独曝光-->
        <snapshotExposure>0</snapshotExposure>
        <!--是否支持带时间上下限的自动曝光，0-不支持，1-支持-->
        <limitedAutoExposure>1</limitedAutoExposure>
        <!--是否支持用户自定义手动曝光时间，0-不支持，1-支持-->
        <customManualExposure>1</customManualExposure>
        <!--是否支持外部同步信号输入，0-不支持，1-支持-->
        <externalSyncInput>0</externalSyncInput>
        <!--是否支持参考电压设置，0-不支持，1-支持-->
        <refrenceLevel>0</refrenceLevel>
        <!--是否支持闪光灯调节，0-不支持，1-支持-->
        <flashAdjust>0</flashAdjust>
        <!--是否支持ircut控制，0-不支持，1-支持-->
        <ircut>1</ircut>
        <!--支持的最小曝光时间-->
        <minExposureTime>1</minExposureTime>
        <!--支持的最大曝光时间-->
        <maxExposureTime>300</maxExposureTime>
        <!--支持的宽动态范围，0-不支持，1-支持数字宽动态（0~3），2-支持真实宽动态（0~100），3-支持数字宽动态（0~1），4-支持数字宽动态（0~100） 5-支持混合宽动态，即数字宽动态+真实宽动态，等级范围0~100-->
        <wideDynamicRange>1</wideDynamicRange>
        <!--支持快门方式的掩码，相应bit位的值为1表示支持该快门模式bit0-支持双快门全帧率，bit1-支持双快门半帧率，bit2-支持三快门半帧率-->
        <doubleExposure>0</doubleExposure>
        <!--抓拍支持的最小曝光时间-->
        <minExposureTime1>0</minExposureTime1>
        <!--抓拍支持的最大曝光时间-->
        <maxExposureTime1>0</maxExposureTime1>
        <!--2A编译时间，对应的UTC(1970-1-1 00:00:00)秒数-->
        <utc>0</utc>
        <!--2Asvn版本号-->
        <version>0</version>
        <!--最大测光区域个数-->
        <meteringRegionCount>0</meteringRegionCount>
        <!--是否支持自动相位，0-不支持，1-支持-->
        <autoSyncPhase>0</autoSyncPhase>
        <!--是否支持亮度补偿，如根据车牌亮度调整图像曝光，0-不支持，1-支持-->
        <brightnessCompensation>1</brightnessCompensation>
        <!--是否支持强光抑制，0-不支持，1-支持-->
        <glareInhibition>1</glareInhibition>
        <!--支持自动曝光模式掩码，详见VideoExposureMode-->
        <exposureMode>0x1b1</exposureMode>
        <!--是否支持电动变焦，0-不支持，1-支持-->
        <electricFocus>1</electricFocus>
        <!--是否支持聚焦峰值计算，0-不支持，1-支持-->
        <afPeak>1</afPeak>
        <!--视频最小曝光时间小数部分，范围：1~99，单位：0.01毫秒-->
        <minExposureTimeFrac>0</minExposureTimeFrac>
        <!--视频最大曝光时间小数部分，范围：1~99，单位：0.01毫秒-->
        <maxExposureTimeFrac>0</maxExposureTimeFrac>
        <!--图片最小曝光时间小数部分，范围：1~99，单位：0.01毫秒-->
        <minExposureTime1Frac>0</minExposureTime1Frac>
        <!--图片最大曝光时间小数部分，范围：1~99，单位：0.01毫秒-->
        <maxExposureTime1Frac>0</maxExposureTime1Frac>
        <!--支持的锐度可调节最大等级，支持 5，100-->
        <eeModeRange>100</eeModeRange>
        <!--是否支持3D降噪，0-不支持，1-支持软件3D降噪，2-FPGA硬件3D降噪-->
        <tridimDenoise>1</tridimDenoise>
        <!--是否支持鱼眼算法，0-不支持，1-支持控件矫正， 2-支持设备矫正， 3-两者都支持-->
        <fishEye>0</fishEye>
        <!--抓拍是否支持帧匹配模式，0-不支持，1-支持-->
        <frameMatch>0</frameMatch>
        <!--是否支持3D降噪参数设置，0-不支持，1-支持,tridimDenoise为1时才有效-->
        <tridimDenoiseDetails>1</tridimDenoiseDetails>
        <!--3D降噪支持类型，0-支持视频和图片，1-只支持视频，2-只支持图片，3-支持视频、图片独立设置，使能通道参考tridimDenoiseEnableMask-->
        <tridimDenoiseMode>0</tridimDenoiseMode>
        <!--最大图片占空比-->
        <maxPicOccupy>
	        <!--整数部分-->
	        <integer>0</integer>
	        <!--小数部分-->
	        <decimal>0</decimal>
        </maxPicOccupy>
        <!--支持解交错,0-不支持，1-支持-->
        <deInterlace>0</deInterlace>
        <!--支持最大减光晕区域个数，0表示不支持-->
        <maxKillHaloRegion>0</maxKillHaloRegion>
        <!--支持通过外部IO来控制ICR切换-->
        <dayNightColorIO>0</dayNightColorIO>
        <!--是否支持同步对焦-->
        <syncFocus>0</syncFocus>
        <!--支持最小减光晕等级-->
        <minKillHaloLevel>0</minKillHaloLevel>
        <!--支持最大减光晕等级-->
        <maxKillHaloLevel>0</maxKillHaloLevel>
        <!--支持的黑电平参数最小值-->
        <minBlackLevel>0</minBlackLevel>
        <!--支持的黑电平参数最大值-->
        <maxBlackLevel>0</maxBlackLevel>
        <!--支持的白电平参数最小值-->
        <minWhiteLevel>0</minWhiteLevel>
        <!--支持的白电平参数最大值-->
        <maxWhiteLevel>0</maxWhiteLevel>
        <!--最大可旋转分辨率的宽-->
        <maxRotatableWidth>1920</maxRotatableWidth>
        <!--最大可旋转分辨率的高-->
        <maxRotatableHeight>1080</maxRotatableHeight>
        <!--支持的Gamma可调节最大等级，支持 5，100，默认为5。ITC设备使用5，IPC设备使用100-->
        <gammaModeRange>100</gammaModeRange>
        <!--是否支持gammaMode调节，0-不支持，1-支持-->
        <gammaMode>1</gammaMode>
        <!--是否支持日夜切换控制，即是否支持日夜切换灵敏度和延迟时间控制，0-不支持，1-支持-->
        <dayNightSwitchCtrl>1</dayNightSwitchCtrl>
        <!--支持的局部白平衡区域个数，为0表示不支持局部白平衡区域-->
        <maxPartWhiteBalanceCount>0</maxPartWhiteBalanceCount>
        <!--是否支持透雾，0-不支持，1-支持-->
        <defog>1</defog>
        <!--是否支持区域聚焦，0-不支持，1-支持-->
        <focusRegion>1</focusRegion>
        <!--是否支持自动调节后焦，0-不支持，1-支持-->
        <abfFunction>0</abfFunction>
        <!--是否支持图像防抖，可按位或叠加，0-不支持，1-支持电子防抖，2-支持光学防抖，4-支持控件防抖-->
        <imageStabilization>1</imageStabilization>
        <!--是否支持动态融合0-不支持，-1支持-->
        <dynamicFusion>0</dynamicFusion>
        <!--是否支持智能红外曝光(防过爆) 0-不支持, 1-支持-->
        <smartIRExposure>0</smartIRExposure>
        <!--自动光圈灵敏度最小值-->
        <minIrisAutoSensitivity>0</minIrisAutoSensitivity>
        <!--自动光圈灵敏度最大值-->
        <maxIrisAutoSensitivity>100</maxIrisAutoSensitivity>
        <!--最大闪光灯延时时间，负数表示提前, 单位：0.01毫秒-->
        <maxFlashDelayTime>0</maxFlashDelayTime>
        <!--3D降噪使能掩码，1表示使能，0表示不使能；bit0：视频时域，bit1：视频空域，bit2：图片时域，bit3：图片空域-->
        <tridimDenoiseEnableMask>0</tridimDenoiseEnableMask>
        <!--是否支持红外模式下车牌模式调节，1-表示支持，0-表示不支持-->
        <irPlateMode>0</irPlateMode>
    </pVideoinCaps>
